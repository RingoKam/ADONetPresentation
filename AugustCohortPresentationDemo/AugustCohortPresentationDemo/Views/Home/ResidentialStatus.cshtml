@model AugustCohortPresentationDemo.Models.ResidentialStatusVM

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ResidentialStatus</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div>
        <table class="table table-condensed table-striped table-bordered" Style="width: 75%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Lodging</th>
                    <th>Ohioian</th>
                </tr>
            </thead>
            @for (int i = 0; i < Model.Name.Count; i++)
            {
                <tr>
                    <td> @Model.Name[i] </td>
                    <td> @Model.Lodging[i] </td>
                    <td> @Model.OhioResid[i] </td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
